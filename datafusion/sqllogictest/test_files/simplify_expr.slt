statement ok
create table t(a int) as values (1), (2), (3);

# test eliminate distinct for min/max in SimplifyExpr since they are equivalent
query II
select min(distinct a), max(distinct a) from t;
----
1 3

query II
select min(a), max(a) from t;
----
1 3

query TT
explain select min(distinct a), max(distinct a) from t;
----
logical_plan
01)Aggregate: groupBy=[[]], aggr=[[MIN(t.a) AS MIN(DISTINCT t.a), MAX(t.a) AS MAX(DISTINCT t.a)]]
02)--TableScan: t projection=[a]
physical_plan
01)AggregateExec: mode=Single, gby=[], aggr=[MIN(DISTINCT t.a), MAX(DISTINCT t.a)]
02)--MemoryExec: partitions=1, partition_sizes=[1]

query TT
explain select min(a), max(a) from t;
----
logical_plan
01)Aggregate: groupBy=[[]], aggr=[[MIN(t.a), MAX(t.a)]]
02)--TableScan: t projection=[a]
physical_plan
01)AggregateExec: mode=Single, gby=[], aggr=[MIN(t.a), MAX(t.a)]
02)--MemoryExec: partitions=1, partition_sizes=[1]
